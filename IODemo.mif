WIDTH=16;
DEPTH=2048;

ADDRESS_RADIX=HEX;
DATA_RADIX=HEX;

CONTENT BEGIN
0000: 1FFA; -- LOADI -6      ; Load 4 (8-bit value)
0001: 9890; -- OUT Ain      ; Write to A register (144)
0002: 1803; -- LOADI 3      ; Load 2 (8-bit value)  
0003: 9891; -- OUT Bin      ; Write to B register (145)
0004: 9895; -- OUT SFDiv     ; Trigger multiplication (146)
0005: 909F; -- IN Result    ; Read result (159)
0006: 9804; -- OUT Hex0     ; Display on Hex0
[0007..07FF] : 0000; -- [empty memory]
END;
